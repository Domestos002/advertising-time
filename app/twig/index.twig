{% extends "layouts/app.twig" %}

{% block pageTitle %}Главная страница{% endblock %}

{% block content %}

    <main class="page-main">
        <section>
            <div class="container">
                <div class="section-inner">
                    <div class="banner-carousel">
                        {#{% for 1 in 1..3  %}#}
                        <div class="banner">
                            <div class="banner-left">
                                <div class="banner-circle"></div>
                                <div class="banner__icon">
                                    <img src="images/podarok.svg" alt="">
                                </div>
                            </div>
                            <div class="banner-right">
                                <div class="text-white">
                                    <div class="mb-15">
                                        <div class="fz25 mb-10"><p>При заказе</p></div>
                                        <div class="fz50 fx fx-left fx-middle list-mr-10">
                                            <p>от</p>
                                            <span class="inline fz70 text-bold">50 000</span>
                                            <p>руб.</p>
                                        </div>
                                    </div>
                                    <div class="">
                                        <div class="fz25 mb-10">
                                            <p>Баннер "Скоро открытие"</p>
                                        </div>
                                        <div class="text-uppercase fz50">
                                            <p>В подарок!</p>
                                        </div>
                                    </div>
                                </div>
                                <a href="#"
                                   class="btn btn-banner btn-primary bg-brand2 btn-primary--bordered btn-primary--left fz21">ПОДРОБНЕЕ</a>
                            </div>
                        </div>
                        {#{% endfor %}#}
                    </div>
                </div>
            </div>
        </section>
        <section>
            <div class="container">
                <div class="section-inner">
                    <div class="banner-carousel">
                        {#{% for 1 in 1..3  %}#}
                        <div class="banner banner__v2">
